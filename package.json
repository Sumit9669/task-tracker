{
  "name": "task-tracker",
  "version": "1.0.0",
  "description": "Application to track user task status",
  "main": "index.js",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "ts-node -r tsconfig-paths/register ./src --env=production",
    "start:dev": "ts-node -r tsconfig-paths/register ./src --env=development",
    "start:prod": "ts-node -r tsconfig-paths/register ./src --env=production",
    "start:staging": "ts-node -r tsconfig-paths/register ./src --env=staging",
    "start:qa": "ts-node -r tsconfig-paths/register ./src --env=qa",
    "start:nodemon": "nodemon --config nodemon.json",
    "test": "nodemon --config nodemon.test.json",
    "clean": "rm -rf dist && mkdir dist",
    "mig": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "mig:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate --name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sumit9669/task-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sumit9669/task-tracker/issues"
  },
  "homepage": "https://github.com/Sumit9669/task-tracker#readme",
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@constants": "dist/constants"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@types/cors": "^2.8.12",
    "@types/lodash": "^4.14.180",
    "@types/moment": "^2.13.0",
    "@types/mysql": "^2.15.21",
    "@types/uuid": "^8.3.4",
    "amqplib": "^0.8.0",
    "axios": "^0.26.1",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-joi-validation": "^5.0.1",
    "file-type": "^17.1.1",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.29.1",
    "mongo-objectid": "^1.2.2",
    "mongodb": "^3.6.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.25",
    "validator": "^13.7.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonfile": "^6.1.0",
    "@types/node": "^17.0.21",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "@types/validator": "^13.7.1",
    "jsonfile": "^6.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  }
}
